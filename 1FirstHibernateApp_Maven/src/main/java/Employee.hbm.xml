<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="info.sscomputers.model.Employee" table="EMPLOYEE">
 <id name="empno" column="EMPNO" type="integer">
  <generator class="assigned"></generator>
 </id> 
 <property name="ename" column="ENAME" type="string" length="20"/>
 <property name="salary" column="SALARY" type="integer" length="20"/>
</class>
</hibernate-mapping>
